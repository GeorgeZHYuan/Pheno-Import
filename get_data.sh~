#!/bin/bash

function getPatient {
	url="$HOST:$PORT/rest/patients/$1"
	local patient = $(curlPheno $url)
	echo $patient
}

function getFamily {
	url="$HOST:$PORT/rest/patients/$1"
	local family = $(curlPheno $url)
	echo $family
}

function curlPheno {
	request="curl -u $USER:$PSWD -X GET $1"
	response=$($request)
	echo $response
	loggit "$request" "$response"
}


