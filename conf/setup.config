# If left unchanged, the following will: 
# 	1. install a local tMDataLoader
# 	2. configure it to connect to PSQL database

# Change only if using your own tMDataLoader
TM_DATALOADER_PATH='install local version'
TM_CONFIG_FILE_PATH='install local version'

# If using the local installion, make sure TRANSMART_DB_USR username is untaken
TRANSMART_DB_HOST='localhost'
TRANSMART_DB_PORT='5432'
TRANSMART_DB_USR='phenoport_dataloader'
TRANSMART_DB_PWD='phenoport_dataloader'

# Tomcat home directory
TOMCAT_HOME=/usr/share/tomcat7

# Rmodules directory name - go to /var/lib/tomcat7/webapps/transmart/plugins/. Use the directory name containing *rdc-modules*
RMODULES_VERSION=rdc-rmodules-16.2

export TM_DATALOADER_PATH TM_CONFIG_FILE_PATH RANSMART_DB_HOST TRANSMART_DB_PORT TRANSMART_DB_USR TRANSMART_DB_PWD TOMCAT_HOME RMODULES_VERSION
